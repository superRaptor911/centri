; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Admob",
"language": "GDScript",
"path": "res://admob-lib/admob.gd"
} ]
_global_script_class_icons={
"Admob": "res://admob-lib/icon.png"
}

[android]

modules="org/godotengine/godot/GodotAdMob"

[application]

config/name="centri"
run/main_scene="res://menus/MainMenu.tscn"
boot_splash/image="res://sprites/qq.png"
config/icon="res://sprites/icon.png"

[autoload]

game_states="*res://objects/global/game_states.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.133333, 0.129412, 0.129412, 1 )
environment/default_environment="res://default_env.tres"
